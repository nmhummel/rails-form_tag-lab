<h3>New Student Form</h3>

<%= form_tag students_path do %>
  <label>Student's first name:</label><br>
  <%= text_field_tag :'student[first_name]' %><br>

  <label>Student's last name:</label><br>
  <%= text_area_tag :'student[last_name]' %><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= submit_tag "Submit Student" %>
<% end %> 

